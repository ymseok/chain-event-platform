{
  "name": "blockchain-event-ingestor",
  "version": "1.0.0",
  "description": "Blockchain event ingestion service for monitoring blockchain networks and dispatching events",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "dev": "tsx watch src/main.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  }
}
